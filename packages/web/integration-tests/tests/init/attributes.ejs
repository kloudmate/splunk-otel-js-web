<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<title>Test init</title>

	<%- renderAgent({applicationName:'custom-app', deploymentEnvironment: 'custom-environment' , globalAttributes:
		{'key1': 'value1' , 'key2' : 'value2' }}) %>
</head>

<body>
	<p>Attributes</p>
</body>
<script>
	var ourTracer = KloudmateRum.provider.getTracer('default');

	function changeAttributes() {
		KloudmateRum.setGlobalAttributes({ 'key1': 'newvalue1' });
		ourTracer.startSpan('attributes-set').end();
	}

	function resetAttributes() {
		KloudmateRum.setGlobalAttributes();
		ourTracer.startSpan('attributes-reset').end();
	}

	KloudmateRum._experimental_addEventListener('global-attributes-changed', (event) => {
		ourTracer.startSpan('attributes-changed').setAttribute(
			'payload',
			JSON.stringify(event.payload)
		).end();
	});
</script>
<a id="clickToChangeAttributes" href="javascript:changeAttributes()">changeAttributes</a>
<a id="clickToResetAttributes" href="javascript:resetAttributes()">resetAttributes</a>

</html>